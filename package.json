{
  "name": "voteDemo",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8081",
  "devDependencies": {
    "react-scripts": "1.1.4",
    "babel-eslint": "^7.2.3",
    "babel-watch": "^2.0.7",
    "concurrently": "^3.5.0",
    "husky": "^0.13.4",
    "lint-staged": "^3.5.1",
    "prettier": "^1.3.1"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "request": "^2.85.0",
    "web3": "0.20.2"
  },
  "scripts": {
    "start": "concurrently \"PORT=8080 react-scripts start\" \"PORT=8081 babel-watch index.js\"",
    "precommit": "lint-staged",
    "build": "react-scripts build && babel index.js -o index-build.js",
    "test": "react-scripts test --env=jsdom",
    "bluemix": "NODE_ENV=production node index-build.js"
  },
  "engines": {
    "node": "4.2.x"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ],
    "index.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  }
}
